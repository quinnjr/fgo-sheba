{
  "name": "fgo-sheba",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered FGO automation for Android",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quinnjr/fgo-sheba.git"
  },
